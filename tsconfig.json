{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es6",
    "module": "commonjs",
    "strict": false,
    "checkJs": false,
    "strictNullChecks": false,  
    "noImplicitAny": false,    
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,                
    "forceConsistentCasingInFileNames": true,
  }
}
