{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "strictNullChecks": false,  
    "noImplicitAny": false,    
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,                    
    "forceConsistentCasingInFileNames": true,
  }
}
